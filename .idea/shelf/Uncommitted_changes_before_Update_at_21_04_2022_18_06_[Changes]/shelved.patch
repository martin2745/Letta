<+>package es.uvigo.esei.daa.rest;\r\n\r\nimport java.util.logging.Level;\r\nimport java.util.logging.Logger;\r\n\r\nimport javax.ws.rs.DELETE;\r\nimport javax.ws.rs.FormParam;\r\nimport javax.ws.rs.GET;\r\nimport javax.ws.rs.POST;\r\nimport javax.ws.rs.PUT;\r\nimport javax.ws.rs.Path;\r\nimport javax.ws.rs.PathParam;\r\nimport javax.ws.rs.Produces;\r\nimport javax.ws.rs.core.MediaType;\r\nimport javax.ws.rs.core.Response;\r\n\r\nimport es.uvigo.esei.daa.dao.DAOException;\r\nimport es.uvigo.esei.daa.dao.rrppDAO;\r\nimport es.uvigo.esei.daa.entities.RRPP;\r\n\r\n/**\r\n * REST resource for managing people.\r\n *\r\n * @author DRM\r\n */\r\n@Path(\"/rrpp\")\r\n@Produces(MediaType.APPLICATION_JSON)\r\npublic class RrppResource {\r\n    private final static Logger LOG = Logger.getLogger(RrppResource.class.getName());\r\n\r\n    private final rrppDAO dao;\r\n\r\n    /**\r\n     * Constructs a new instance of {@link PeopleResource}.\r\n     */\r\n    public RrppResource() {\r\n        this(new rrppDAO());\r\n    }\r\n\r\n    // Needed for testing purposes\r\n    RrppResource(rrppDAO dao) {\r\n        this.dao = dao;\r\n    }\r\n\r\n    /**\r\n     * Returns a person with the provided identifier.\r\n     *\r\n     * @param id the identifier of the person to retrieve.\r\n     * @return a 200 OK response with a person that has the provided identifier.\r\n     * If the identifier does not corresponds with any user, a 400 Bad\r\n     * Request\r\n     * response with an error message will be returned. If an error happens\r\n     * while retrieving the list, a 500 Internal Server Error response with\r\n     * an\r\n     * error message will be returned.\r\n     */\r\n    @GET\r\n    @Path(\"/{id}\")\r\n    public Response get(\r\n            @PathParam(\"id\") int id) {\r\n        try {\r\n            final RRPP rrpp = this.dao.get(id);\r\n\r\n            return Response.ok(rrpp).build();\r\n        } catch (IllegalArgumentException iae) {\r\n            LOG.log(Level.FINE, \"I