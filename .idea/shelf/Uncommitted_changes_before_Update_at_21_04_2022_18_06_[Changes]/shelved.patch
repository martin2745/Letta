<+>var EventsSubsView = (function () {\r\n    var dao;\r\n    const rDAO = new rrppDAO();\r\n    // Referencia a this que permite acceder a las funciones públicas desde las funciones de jQuery.\r\n    var self;\r\n    var formId = 'events-form';\r\n    var listId = 'events-list';\r\n    var formQuery = '#' + formId;\r\n    var listQuery = '#' + listId;\r\n\r\n    function EventsSubsView(eventDao, formContainerId, listContainerId) {\r\n        dao = eventDao;\r\n\r\n        self = this;\r\n        var login = getCookie(\"login\");\r\n        var id = eventDao.getId(login,function (person){\r\n            console.log(login)\r\n            id = person.id;\r\n        });\r\n\r\n        rDAO.getRrpp(id,function (rrpp){\r\n            console.log(rrpp);\r\n        })\r\n\r\n        insertEventList($('#' + listContainerId));\r\n\r\n        this.init = function () {\r\n            dao.listEventSubs(function (people) {\r\n                    $.each(people, function (key, event) {\r\n                        appendToTable(event);\r\n                    });\r\n                },\r\n                function () {\r\n                    alert('No has sido posible acceder al listado de eventos.');\r\n                });\r\n        };\r\n\r\n        this.getEventInForm = function () {\r\n            var form = $(formQuery);\r\n            return {\r\n                'id': form.find('input[name=\"id\"]').val(),\r\n                'ubicacion': form.find('input[name=\"ubicacion\"]').val(),\r\n                'nombre': form.find('input[name=\"nombre\"]').val(),\r\n                'descripcion': form.find('input[name=\"descripcion\"]').val(),\r\n                'date': form.find('input[name=\"date\"]').val()\r\n            };\r\n        };\r\n\r\n        this.getEventInRow = function (id) {\r\n            var row = $('#event-' + id);\r\n\r\n            if (row !== undefined) {\r\n                return {\r\n                    'id': id,\r\n                    'ubicacion': row.find('td.ubicacion').text(),\r\n                    'nombre': row.find('td.nombre').text(),\r\n                    'descripcion': row.find('td.descripcion').text(),\r\n                    'date': row.find('td.date').text()\r\n                };\r\n            } else {\r\n                return undefined;\r\n            }\r\n        };\r\n\r\n        this.disableForm = function () {\r\n            $(formQuery + ' input').prop('disabled', true);\r\n        };\r\n\r\n        this.enableForm = function () {\r\n            $(formQuery + ' input').prop('disabled', false);\r\n        };\r\n\r\n        this.resetForm = function () {\r\n            $(formQuery)[0].reset();\r\n            $(formQuery + ' input[name=\"id\"]').val('');\r\n            $('#btnSubmit').val('Crear');\r\n        };\r\n    };\r\n\r\n    var insertEventList = function (parent) {\r\n        parent.append(\r\n            '<table id=\"' + listId + '\" class=\"table\">\\\r\n\t\t\t\t<thead>\\\r\n\t\t\t\t\t<tr class=\"row\">\\\r\n\t\t\t\t\t\t<th class=\"col-sm-3\">Ubicación</th>\\\r\n\t\t\t\t\t\t<th class=\"col-sm-2\">Nombre</th>\\\r\n\t\t\t\t\t\t<th class=\"col-sm-3\">Descripción</th>\\\r\n\t\t\t\t\t\t<th class=\"col-sm-2\">Fecha</th>\\\r\n\t\t\t\t\t\t<th class=\"col-sm-2\">&nbsp;</th>\\\r\n\t\t\t\t\t</tr>\\\r\n\t\t\t\t</thead>\\\r\n\t\t\t\t<tbody>\\\r\n\t\t\t\t</tbody>\\\r\n\t\t\t</table>'\r\n        );\r\n    };\r\n\r\n    var createEventRow = function (event) {\r\n        return '<tr id=\"event-' + event.id + '\" class=\"row\">\\\r\n\t\t\t<td class=\"ubicacion col-sm-3\">' + event.ubicacion + '</td>\\\r\n\t\t\t<td class=\"nombre col-sm-2\">' + event.nombre + '</td>\\\r\n\t\t\t<td class=\"descripcion col-sm-3\">' + event.descripcion + '</td>\\\r\n\t\t\t<td class=\"date col-sm-2\">' + event.date + '</td>\\\r\n\t\t\t<td class=\"col-sm-2\">\\\r\n\t\t\t\t<a class=\"edit btn btn-primary\" href=\"#\">Suscribirse</a>\\\r\n\t\t\t</td>\\\r\n\t\t\t<td class=\"col-sm-2\">\\\r\n\t\t\t\t<a class=\"edit btn btn-primary\" href=\"#\">Enviar</a>\\\r\n\t\t\t</td>\\\r\n\t\t</tr>';\r\n    };\r\n\r\n    var showErrorMessage = function (jqxhr, textStatus, error) {\r\n        alert(textStatus + \": \" + error);\r\n    };\r\n\r\n    var addRowListeners = function (event) {\r\n        $('#event-' + event.id + ' a.edit').click(function () {\r\n            self.editEvent(event.id);\r\n        });\r\n\r\n        $('#event-' + event.id + ' a.delete').click(function () {\r\n            self.deleteEvent(event.id);\r\n        });\r\n    };\r\n\r\n    var appendToTable = function (event) {\r\n        $(listQuery + ' > tbody:last')\r\n            .append(createEventRow(event));\r\n        addRowListeners(event);\r\n    };\r\n\r\n    return EventsSubsView;\r\n})();\r\n
